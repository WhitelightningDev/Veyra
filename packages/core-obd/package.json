{
  "name": "core-obd",
  "version": "0.1.0",
  "description": "Reusable ELM327/OBD-II logic for the mobile app",
  "private": false,
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --declaration --rootDir src --outDir dist --target ES2019 --module CommonJS --strict --esModuleInterop --skipLibCheck",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "obd",
    "elm327",
    "can",
    "iso15765",
    "uds"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.6.3",
    "rimraf": "^6.0.1"
  }
  
}
